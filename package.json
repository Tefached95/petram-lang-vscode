{
  "name": "petram-lang",
  "displayName": "Petram Language Support",
  "description": "Syntax highlighting and language support for Petram",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "petram",
        "aliases": [
          "Petram",
          "petram"
        ],
        "extensions": [
          ".petra"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "petram",
        "scopeName": "source.petra",
        "path": "./syntaxes/petram.tmLanguage.json"
      }
    ]
  }
}